<Page x:Class="CUOnline.StudentFeePage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
      xmlns:local="clr-namespace:CUOnline"
      mc:Ignorable="d"
      d:DesignHeight="720"
      d:DesignWidth="1250"
      Title="StudentFeePage">

    <Grid Background="{StaticResource WhiteColour}">
        <Label Content="Fees and Dues"
               Style="{StaticResource Heading1Style}"
               Margin="50 20 0 0" />
        <DataGrid d:ItemsSource="{d:SampleData ItemCount=5}"
                  Height="100"
                  Width="1050"
                  VerticalAlignment="Top"
                  Margin="0 150 0 0" />


    </Grid>
</Page>
